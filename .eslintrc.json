{
  "extends": ["next/babel", "next/core-web-vitals"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn", { "args": "none" }],
    "import/no-unresolved": "error",
    "import/named": "off",
    "no-console": "warn"
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
